{
    "manifest_version": 3,
    "name": "E-fellows tag blocker",
    "version": "1.0.0",
    "description": "This is a chrome extension that allows blocking tags on the e-fellows community forum    ",
    "short_name": "EF tag blocker",
    "content_scripts": [
      {
        "matches": ["https://www.community.e-fellows.net/*"],
        "js": ["contentscript.js"]
      }
    ],
    "action": {
      "default_title": "Does a thing when you do a thing",
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.jpg"
      }
    },
    "icons": {
       "16": "icons/icon16.jpg"
     }
   }
